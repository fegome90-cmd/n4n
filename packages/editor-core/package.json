{
  "name": "@n4n/editor-core",
  "version": "1.0.0",
  "description": "Editor core with modes and state management for N4N Engine",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --no-dts",
    "dev": "tsup src/index.ts --format cjs,esm --no-dts --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "keywords": [
    "editor",
    "neovim",
    "keyboard-first",
    "modes",
    "state"
  ],
  "author": "N4N Engine Team",
  "license": "MIT"
}
